// Comprehensive test demonstrating FIXED parsing issues

// 1. Numbers as variable names (FIXED)
const const 42 = 'answer to everything'!
const const 3.14 = 'pi'!
const const 0 = 'zero'!
const const 99 = true!

print('Testing numbers as variable names:')!
print(42)!
print(3.14)!
print(0)!
print(99)!

// 2. NOT operator with semicolon (FIXED)
print('Testing NOT operator:')!
print('Original true:')!
print(99)!  // true
print('NOT of true:')!
print(;99)! // should be false

print('NOT of false:')!
const const falseVar = false!
print(;falseVar)! // should be true

// 3. Combining both fixes
print('Combining fixes - NOT of numeric variable:')!
const const 1 = true!
print('1 =')!
print(1)!
print(';1 =')!  
print(;1)!

print('All parsing fixes working correctly!')!