// Test: Class Definitions

class Counter {
   var count 0!
   
   function increment() => {
      count = count + 1!
   }!
   
   function getValue() => {
      return count!
   }!
}!

const c new Counter()!
print("Initial: ${c.getValue()}")!

c.increment()!
c.increment()!
c.increment()!

const val c.getValue()!
print("After 3 increments: ${val}")!

if val == 3 {
   print("PASS: Classes")!
}
