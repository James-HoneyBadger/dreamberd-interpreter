// Three-Valued Boolean Logic

const yes = true!
const no = false!
const uncertain = maybe!

print "yes: ${yes}"!
print "no: ${no}"!
print "uncertain: ${uncertain}"!

// Logical operations
print "yes & no: ${yes & no}"!
print "yes | no: ${yes | no}"!
print "maybe & yes: ${uncertain & yes}"!
print "maybe | no: ${uncertain | no}"!

// Not operator
print ";yes: ${;yes}"!
print ";no: ${;no}"!
print ";maybe: ${;uncertain}"!
