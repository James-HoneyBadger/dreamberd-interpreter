// Arithmetic Operations

const a = 10!
const b = 3!

print "Addition: ${a} + ${b} = ${a + b}"!
print "Subtraction: ${a} - ${b} = ${a - b}"!
print "Multiplication: ${a} * ${b} = ${a * b}"!
print "Division: ${a} / ${b} = ${a / b}"!
print "Power: ${a} ^ ${b} = ${a ^ b}"!

// Complex expressions
const result = (a + b) * 2 ^ 3!
print "(${a} + ${b} * 2 ^ 3 = ${result}")!
