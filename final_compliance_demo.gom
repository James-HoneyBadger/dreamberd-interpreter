// ✅ 2. VARIABLE DECLARATION SYSTEM - Four types as specified  
print "2. Variable declaration system..."!
const const immutable = "cannot change"!
const var editable = "can edit"!
var const reassignable = "can reassign"! 
var var flexible = "can do both"!
print "All declaration types:" immutable editable reassignable flexible!

// ✅ 3. ARRAYS START AT INDEX -1 - Unique Gulf of Mexico feature
print "3. Arrays starting at -1..."!
var testScores = [10, 20, 30]!
print "testScores[-1] =" testScores[-1]!  // Should be 10
print "testScores[0] =" testScores[0]!    // Should be 20
print "testScores[1] =" testScores[1]!    // Should be 30

// ✅ 4. FLOAT INDEXING - Insert values at fractional indices
print "4. Float array indexing..."!
