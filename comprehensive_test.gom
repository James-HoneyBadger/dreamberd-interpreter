// Comprehensive Gulf of Mexico Interpreter Test
// This test covers all major features of the interpreter

print "=== Gulf of Mexico Interpreter Comprehensive Test ==="!

// 1. Variable declarations and assignments
print "1. Testing variable declarations..."!
var x = 42!
const pi = 3.14159!
var message = "Hello Gulf of Mexico"!
var flag = true!

print "Variables declared: x =" x ", pi =" pi ", message =" message ", flag =" flag!

// 2. Array operations
print "2. Testing arrays..."!
var numbers = [1, 2, 3, 4, 5]!
var mixed = [42, "text", true, [1, 2]]!
print "numbers array:" numbers!
print "mixed array:" mixed!

// 3. Mathematical operations  
print "3. Testing mathematical operations..."!
var sum = x + 8!
var product = x * 2!
var quotient = x / 6!
print "x + 8 =" sum ", x * 2 =" product ", x / 6 =" quotient!

// 4. String operations
print "4. Testing string operations..."!
var greeting = "Hello" + " " + "World"!
print "String concatenation:" greeting!

// 5. Boolean operations
print "5. Testing boolean operations..."!
var and_result = true & false!
var or_result = true | false!
var not_result = ;true!
print "true & false =" and_result ", true | false =" or_result ", ;true =" not_result!

// 6. Comparison operations
print "6. Testing comparisons..."!
var eq = x == 42!
var neq = x != 50!
var gt = x > 40!
var lt = x < 50!
print "x == 42:" eq ", x != 50:" neq ", x > 40:" gt ", x < 50:" lt!

// 7. Function definitions and calls
print "7. Testing functions..."!
function add(a, b) {
    var result = a + b!
    return result!
}!

function greet(name) {
    return "Hello, " + name + "!"!
}!

var sum_result = add(10, 5)!
var greeting_result = greet("Gulf of Mexico")!
print "add(10, 5) =" sum_result!
print "greet result:" greeting_result!

// 8. Variable reassignment and reverse
print "8. Testing variable modification and reverse..."!
var test_var = 100!
print "Original test_var:" test_var!
test_var = 200!
print "Modified test_var:" test_var!
reverse!
print "After reverse, test_var:" test_var!

// 9. Constructor calls
print "9. Testing constructor calls..."!
var obj = new Object()!
print "Constructor called successfully"!

// 10. Print statement variations
print "10. Testing print variations..."!
print "Single argument"!
print "Multiple" "arguments" "in" "one" "statement"!

// 11. Export variables for import testing
print "11. Exporting variables..."!
export x, pi, message, numbers!
print "Variables exported successfully"!

print "=== All tests completed successfully! ==="!